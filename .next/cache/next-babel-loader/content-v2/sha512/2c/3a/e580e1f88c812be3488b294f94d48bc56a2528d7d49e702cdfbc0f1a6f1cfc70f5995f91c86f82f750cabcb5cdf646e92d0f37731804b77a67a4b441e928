{"ast":null,"code":"import { createSlice } from '@reduxjs/toolkit';\nimport { getUserData } from './thunk';\nconst initialState = {\n  data: null,\n  loading: false,\n  error: null\n};\nexport const userDataSlice = createSlice({\n  name: 'userData',\n  initialState,\n  reducers: {},\n  extraReducers: builder => {\n    builder.addCase(getUserData.pending, (state, {\n      payload\n    }) => {\n      state.data = null;\n      state.loading = true;\n      state.error = null;\n    });\n    builder.addCase(getUserData.fulfilled, (state, {\n      payload\n    }) => {\n      state.data = payload;\n      state.loading = false;\n      state.error = null;\n    });\n    builder.addCase(getUserData.rejected, (state, action) => {\n      if (action.payload) {\n        state.error = action.payload;\n      } else {\n        state.error = action.error;\n      }\n\n      state.loading = false;\n    });\n  }\n});","map":{"version":3,"sources":["C:/Users/Digicrym/Documents/inspocreate/src/reducers/user/slice.js"],"names":["createSlice","getUserData","initialState","data","loading","error","userDataSlice","name","reducers","extraReducers","builder","addCase","pending","state","payload","fulfilled","rejected","action"],"mappings":"AAAA,SAASA,WAAT,QAA4B,kBAA5B;AACA,SAASC,WAAT,QAA4B,SAA5B;AAEA,MAAMC,YAAY,GAAG;AACpBC,EAAAA,IAAI,EAAE,IADc;AAEpBC,EAAAA,OAAO,EAAE,KAFW;AAGpBC,EAAAA,KAAK,EAAE;AAHa,CAArB;AAMA,OAAO,MAAMC,aAAa,GAAGN,WAAW,CAAC;AACxCO,EAAAA,IAAI,EAAE,UADkC;AAExCL,EAAAA,YAFwC;AAGxCM,EAAAA,QAAQ,EAAE,EAH8B;AAIxCC,EAAAA,aAAa,EAAGC,OAAD,IAAa;AAC3BA,IAAAA,OAAO,CAACC,OAAR,CAAgBV,WAAW,CAACW,OAA5B,EAAqC,CAACC,KAAD,EAAQ;AAAEC,MAAAA;AAAF,KAAR,KAAwB;AAC5DD,MAAAA,KAAK,CAACV,IAAN,GAAa,IAAb;AACAU,MAAAA,KAAK,CAACT,OAAN,GAAgB,IAAhB;AACAS,MAAAA,KAAK,CAACR,KAAN,GAAc,IAAd;AACA,KAJD;AAKAK,IAAAA,OAAO,CAACC,OAAR,CAAgBV,WAAW,CAACc,SAA5B,EAAuC,CAACF,KAAD,EAAQ;AAAEC,MAAAA;AAAF,KAAR,KAAwB;AAC9DD,MAAAA,KAAK,CAACV,IAAN,GAAaW,OAAb;AACAD,MAAAA,KAAK,CAACT,OAAN,GAAgB,KAAhB;AACAS,MAAAA,KAAK,CAACR,KAAN,GAAc,IAAd;AACA,